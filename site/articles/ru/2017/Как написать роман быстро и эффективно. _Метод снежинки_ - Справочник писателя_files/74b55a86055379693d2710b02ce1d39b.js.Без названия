jQuery(function(){if(jQuery("#slidebox").length>=1){var a=jQuery("html").height()>window.innerHeight;a||jQuery("#slidebox").stop(!0).animate({right:"0px"},300),jQuery(window).scroll(function(){var a=jQuery(window).scrollTop()+jQuery(window).height()+1,b=jQuery(document).height(),c=slidein_settings.options.slidein_percentage/100*b;a>=c?jQuery("#slidebox").stop(!0).animate({right:"0px"},300):jQuery("#slidebox").stop(!0).animate({right:"-530px"},100)}),jQuery("#slidebox .close").bind("click",function(){jQuery("#slidebox").animate({right:"-530px"},300,function(){jQuery("#slidebox").remove()})})}});;jQuery(function(){jQuery(":input").on("focus",function(){var input=jQuery(this);var inputID=input.attr("id")||"(no input ID)";var inputName=input.attr("name")||"(no input name)";var inputClass=input.attr("class")||"(no input class)";var form=jQuery(this.form);var formID=form.attr("id")||"(no form ID)";var formName=form.attr("name")||"(no form name)";var formClass=form.attr("class")||"(no form class)";window[gtm4wp_datalayer_name].push({'event':'gtm4wp.formElementEnter','inputID':inputID,'inputName':inputName,'inputClass':inputClass,'formID':formID,'formName':formName,'formClass':formClass});}).on("blur",function(){var input=jQuery(this);var inputID=input.attr("id")||"(no input ID)";var inputName=input.attr("name")||"(no input name)";var inputClass=input.attr("class")||"(no input class)";var form=jQuery(this.form);var formID=form.attr("id")||"(no form ID)";var formName=form.attr("name")||"(no form name)";var formClass=form.attr("class")||"(no form class)";window[gtm4wp_datalayer_name].push({'event':'gtm4wp.formElementLeave','inputID':inputID,'inputName':inputName,'inputClass':inputClass,'formID':formID,'formName':formName,'formClass':formClass});});});